{
    "name": "koenvanmeijeren/koenavel",
    "authors": [
        {
            "name": "Koen van Meijeren",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require": {
        "defuse/php-encryption": "^2.2",
        "paragonie/anti-csrf": "^2.2",
        "monolog/monolog": "^2.0",
        "siriusphp/upload": "^2.1",
        "php": "^7.3",
        "cakephp/chronos": "^1.2",
        "kminek/email-obfuscator": "^1.0",
        "filp/whoops": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.11.7",
        "squizlabs/php_codesniffer": "^3.2",
        "php-coveralls/php-coveralls": "^2.1",
        "thecodingmachine/phpstan-strict-rules": "^0.11.2",
        "vimeo/psalm": "^3.5"
    },
    "scripts": {
        "phpunit": "phpunit",
        "phpstan": "phpstan analyse app -c phpstan.neon --level=7",
        "cs-fix": "phpcbf",
        "cs-check": "phpcs"
    }
}
